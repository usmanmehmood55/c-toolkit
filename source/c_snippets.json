{
    "Add packed attribute": 
    {
        "prefix"     : "packed",
        "description": "Adds a 'packed' attribute",
        "body"       : 
        [
            "__attribute__ ((__packed__))"
        ]
    },
    "Add aligned attribute": 
    {
        "prefix"     : "aligned",
